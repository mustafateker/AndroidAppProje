<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    tools:ignore="ExtraText">


        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="75dp"
            android:layout_height="wrap_content"
            android:src="@drawable/gecmis_denemeler_histogram_icon"
            android:scaleType="fitCenter"
            android:layout_margin="2dp"
            />


        <TextView
            android:id="@+id/siraIndeksiTextView"
            android:layout_width="31dp"
            android:layout_height="24dp"
            android:gravity="center_vertical"
            android:text="Sıra İndeksi" />

        <TextView
            android:id="@+id/adiTextView"
            android:layout_width="139dp"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text="Adı" />

        />

        <TextView
            android:id="@+id/tarihTextView"
            android:layout_width="93dp"
            android:layout_height="27dp"
            android:gravity="center_vertical"
            android:text="Tarih" />

        <TextView
            android:id="@+id/genelNetBilgisiTextView"
            android:layout_width="67dp"
            android:layout_height="50dp"
            android:gravity="center_vertical"
            android:text="Genel Net Bilgisi" />
</LinearLayout>


