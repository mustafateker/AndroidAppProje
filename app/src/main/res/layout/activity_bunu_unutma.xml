<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".bunu_unutma">


  <ImageView
      android:id="@+id/main_top_fon_ImageView"
      android:layout_width="wrap_content"
      android:layout_height="85dp"
      android:layout_alignParentTop="true"
      android:contentDescription="@string/anamenutop"
      android:scaleType="fitXY"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.0"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:srcCompat="@drawable/main_top_fon_revize"
      tools:ignore="ImageContrastCheck" />

  <ImageView
      android:id="@+id/performansim_app_logo"
      android:layout_width="88dp"
      android:layout_height="82dp"
      android:layout_marginStart="4dp"
      android:layout_marginEnd="15dp"
      android:contentDescription="app logo"
      app:layout_constraintBottom_toBottomOf="@id/main_top_fon_ImageView"
      app:layout_constraintEnd_toStartOf="@+id/textView15"
      app:layout_constraintStart_toStartOf="@id/main_top_fon_ImageView"
      app:layout_constraintTop_toTopOf="@+id/textView15"
      app:layout_constraintVertical_bias="0.0"
      app:srcCompat="@drawable/app_icon__main_top"
      tools:ignore="HardcodedText" />

  <TextView
      android:id="@+id/textView15"
      android:layout_width="wrap_content"
      android:layout_height="85dp"
      android:layout_marginStart="20dp"
      android:gravity="center_vertical"
      android:text="@string/bunu_unutma"
      android:textColor="@color/white"
      android:textSize="39sp"
      app:layout_constraintBottom_toBottomOf="@+id/main_top_fon_ImageView"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.493"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0" />

  <TextView
      android:id="@+id/textView4"
      android:layout_width="266dp"
      android:layout_height="48dp"
      android:layout_marginStart="12dp"
      android:layout_marginTop="143dp"
      android:layout_marginBottom="144dp"
      android:gravity="center_vertical"
      android:text="Günlük soru hedefini tamamladın mı?"
      android:textSize="16sp"
      app:layout_constraintBottom_toTopOf="@+id/textView5"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.0"
      tools:ignore="HardcodedText,TextSizeCheck" />

  <CheckBox
      android:id="@+id/checkbox"
      android:layout_width="104dp"
      android:layout_height="0dp"
      android:layout_marginTop="56dp"
      android:text="@string/z_ld"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.724"
      app:layout_constraintStart_toEndOf="@+id/textView4"
      app:layout_constraintTop_toBottomOf="@+id/main_top_fon_ImageView" />

  <TextView
      android:id="@+id/textView5"
      android:layout_width="148dp"
      android:layout_height="36dp"
      android:layout_marginStart="12dp"
      android:text="Günlük soru hedefi"
      android:textSize="16sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.44"
      tools:ignore="HardcodedText,TextSizeCheck" />

  <TextView
      android:id="@+id/textView6"
      android:layout_width="41dp"
      android:layout_height="26dp"
      android:layout_marginStart="65dp"
      android:layout_marginEnd="67dp"
      android:layout_marginBottom="56dp"
      android:text="TYT"
      app:layout_constraintBottom_toBottomOf="@+id/editTextNumber"
      app:layout_constraintEnd_toStartOf="@+id/textView7"
      app:layout_constraintStart_toEndOf="@+id/textView5"
      tools:ignore="HardcodedText,TextSizeCheck" />

  <TextView
      android:id="@+id/textView7"
      android:layout_width="41dp"
      android:layout_height="0dp"
      android:text="AYT"
      app:layout_constraintBottom_toBottomOf="@+id/textView6"
      app:layout_constraintEnd_toEndOf="@+id/editTextNumber2"
      app:layout_constraintStart_toStartOf="@+id/editTextNumber2"
      app:layout_constraintTop_toTopOf="@+id/textView6"
      tools:ignore="HardcodedText" />


  <EditText
      android:id="@+id/editTextNumber2"
      android:layout_width="0dp"
      android:layout_height="48dp"
      android:layout_marginTop="88dp"
      android:autofillHints=""
      android:ems="10"
      android:hint="Sayi Giriniz"
      android:inputType="number"
      android:textColorHint="#546E7A"
      app:layout_constraintBottom_toTopOf="@+id/switch1"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/editTextNumber"
      app:layout_constraintTop_toBottomOf="@+id/checkbox"
      app:layout_constraintVertical_bias="0.174"
      tools:ignore="HardcodedText" />

  <EditText
      android:id="@+id/editTextNumber"
      android:layout_width="0dp"
      android:layout_height="48dp"
      android:layout_marginEnd="10dp"
      android:autofillHints=""
      android:ems="10"
      android:hint="Sayı Giriniz"
      android:inputType="number"
      android:textColorHint="#546E7A"
      app:layout_constraintBaseline_toBaselineOf="@+id/editTextNumber2"
      app:layout_constraintEnd_toStartOf="@+id/editTextNumber2"
      app:layout_constraintStart_toEndOf="@+id/textView5"
      tools:ignore="HardcodedText" />

  <TextView
      android:id="@+id/textView11"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_marginStart="12dp"
      android:gravity="center_vertical"
      android:text="Günlük hedeflenen soru sayısı girilmediğinde uyarı al"
      app:layout_constraintBottom_toBottomOf="@+id/switch1"
      app:layout_constraintEnd_toStartOf="@+id/switch1"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="@+id/switch1"
      tools:ignore="HardcodedText" />


  <Switch
      android:id="@+id/switch1"
      android:layout_width="112dp"
      android:layout_height="74dp"
      android:layout_marginBottom="244dp"
      android:text="Uyarı al "
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toEndOf="@+id/textView11"
      tools:ignore="HardcodedText" />

  <RatingBar
      android:id="@+id/ratingBar"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginEnd="8dp"
      android:layout_marginBottom="136dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent" />

  <TextView
      android:id="@+id/textView9"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginStart="12dp"
      android:layout_marginTop="12dp"
      android:text="Güne Puanın ?"
      android:textSize="20sp"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="@+id/ratingBar"
      tools:ignore="HardcodedText" />


</androidx.constraintlayout.widget.ConstraintLayout>